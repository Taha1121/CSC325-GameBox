<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="476.0" prefWidth="570.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.gamebox.RegisterPageController">
    <children>
        <Label text="Welcome!" />
        <TextField fx:id="registeremailTextField" promptText="Email Address" />
        <TextField fx:id="registernameTextField" promptText="Name" />
        <TextField fx:id="registerphoneTextField" promptText="Phone Number" />
        <TextField fx:id="registerpasswordTextField" promptText="Password" />
        <Button fx:id="registrationButton" mnemonicParsing="false" onAction="#registerButtonClicked" text="Register" />
        <Button fx:id="goBackbtn" mnemonicParsing="false" text="SignInPage" onAction="#goBackToLoginPage" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
